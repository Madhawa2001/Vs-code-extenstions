{
  "name": "code-analyzer-offline",
  "displayName": "code-analyzer-offline",
  "description": "Analyzes code and generates a rationale document",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rationale.generateForFile",
        "title": "Generate Rationale Document for Active File"
      },
      {
        "command": "rationale.generateForWorkspace",
        "title": "Generate Rationale Document for Workspace"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "rationale.generateForFile",
          "group": "navigation"
        },
        {
          "command": "rationale.generateForWorkspace",
          "group": "navigation"
        }
      ]
    }
  },
  "files": [
    "dist/**",
    "src/analyzers/python_analyzer.py"
  ],
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "npm run compile",
    "pretest": "npm run build",
    "test": "node ./out/test/runTests.js"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@babel/traverse": "^7.28.0"
  }
}
